  static void Main()
  {
      int[] numbers = new int[5];
      for (int i = 0; i < 5; i++)
      {
          Console.Write("Please enter a number: ");
          numbers[i] = Convert.ToInt32(Console.ReadLine());
      }
      Console.WriteLine("The numbers you entered are:");
      foreach (int number in numbers)
      {
          Console.WriteLine(number);
      }
  }
